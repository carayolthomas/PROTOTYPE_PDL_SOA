<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="database.CustomerDWH" table="Customer">
        <id name="mIdCustomerDWH" type="java.lang.Integer">
            <column name="idCustomer" />
            <generator class="identity" />
        </id>
        <property name="mFirstNameCustomerDWH" type="string">
            <column name="firstNameCustomer" length="50" not-null="true" />
        </property>
        <property name="mLastNameCustomerDWH" type="string">
            <column name="lastNameCustomer" length="50" not-null="true" />
        </property>
        <property name="mEmailCustomerDWH" type="string">
            <column name="emailCustomer" length="100" not-null="true" />
        </property>
        <property name="mAdressCustomerDWH" type="string">
            <column name="adressCustomer" length="100" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
